{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext"],
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"noEmit": false,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"checkJs": false
	}
}
